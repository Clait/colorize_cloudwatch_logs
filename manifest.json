{
  "name": "Colorize CloudWatch Logs",
  "description" : "Group visually logs on AWS CloudWatch",
  "version": "0.3.7",
  "homepage_url": "https://github.com/ilhan-mstf/colorize_cloudwatch_logs",
  "manifest_version": 2,
  "icons": {
    "16": "img/logo_16.png",
    "32": "img/logo_32.png",
    "64": "img/logo_64.png",
    "128": "img/logo_128.png"
  },
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "img/logo_16.png"
  },
  "content_scripts": [{
    "matches": [
      "https://*.console.aws.amazon.com/cloudwatch/*"
    ],
    "js": [
      "lib/ainsi_up.js",
      "colorize.js"
    ]
  }]
}
